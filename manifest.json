{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "habla_icon.png",
    "default_popup": "habla_popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": ["test.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}