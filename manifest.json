{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "habla_icon.png"
  },
  "background": {
    "scripts": ["habla-background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["styles.css"],
      "js": ["jquery-3.1.1.min.js", "habla-base.js", "habla-helpers.js", "habla-content-script.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}